function, of = f / values, map = (
    if, condition = (first, of = (value, of = values)),
        then = ( list, values = (f, values = (first, of = (value, of = values))) 
                               / (map, f = (value, of = f),
                                       values = (rest, of = (value, of = values))))
)

test, label = pass a native function to a map function,
do = ( map, f = (value, of = println), values = 1 / 2 / 3 ),
expect = 1 / 2 / 3

test, label = pass a user defined function to a map function,
do = ( 
        function, of = values, foo = (
            println, values = Hello / (value, of = values) 
        )
        map, f = (value, of = foo), values = 1 / 2 / 3
     ),
expect = Hello / 1 / Hello / 2 / Hello / 3

